#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sapiens19/rentalx/node_modules/.pnpm/typeorm@0.3.17/node_modules/typeorm/node_modules:/home/sapiens19/rentalx/node_modules/.pnpm/typeorm@0.3.17/node_modules:/home/sapiens19/rentalx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sapiens19/rentalx/node_modules/.pnpm/typeorm@0.3.17/node_modules/typeorm/node_modules:/home/sapiens19/rentalx/node_modules/.pnpm/typeorm@0.3.17/node_modules:/home/sapiens19/rentalx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
